extends layout

block login
  div(align='center')
    label Totalï¼š #{data.length}
  .login-clean
    form(method='post', style='background-color:rgba(255,255,255,0.8);')
      h2.sr-only Login Form
      .illustration
        i.icon.ion-ios-navigate
      .form-group
        input.form-control(type='username', name='userID', placeholder='User ID', value=180001)
